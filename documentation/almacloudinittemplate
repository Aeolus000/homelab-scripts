qm create 9000 \
  --name alma9-cloud-template \
  --memory 2048 \
  --cores 2 \
  --net0 virtio,bridge=vmbr0 \
  --bios seabios \
  --ostype l26

qm importdisk 9000 /root/AlmaLinux-9-GenericCloud-latest.x86_64.qcow2 local-lvm

qm set 9000 --sata0 local-lvm:vm-9000-disk-0
qm set 9000 --boot c --bootdisk sata0

qm set 9000 --ide2 local-lvm:cloudinit

qm set 9000 --serial0 socket --vga serial0

qm set 9000 --cpu host

qm config 9000

qm template 9000
